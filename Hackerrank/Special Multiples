import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        long lcm = 2329089562800L; // Precomputed LCM of primes â‰¤ 40
        while (t-- > 0) {
            long n = sc.nextLong();
            if (n % lcm == 0) System.out.println("YES");
            else System.out.println("NO");
        }
    }
}
